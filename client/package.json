{
  "name": "react-template",
  "version": "0.0.1",
  "description": "",
  "engines": {
    "npm": ">=3"
  },
  "proxy": "http://localhost:4000",
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm run start-message",
    "start": "concurrently -k -r -s first \"npm run open:src\" \"npm run lint:watch\"",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run lint && npm run clean-dist",
    "build": "babel-node tools/build.js",
    "test": "jest --coverage",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "license": "MIT",
  "dependencies": {
    "amqplib": "0.5.2",
    "ant-design-pro": "1.3.0",
    "antd": "3.19.1",
    "axios": "0.18.0",
    "copy-webpack-plugin": "4.5.2",
    "d3": "^5.5.0",
    "d3-color": "1.2.0",
    "d3-interpolate": "1.2.0",
    "load-google-maps-api": "1.3.2",
    "lodash.clonedeepwith": "4.5.0",
    "lodash.countby": "4.6.0",
    "lodash.debounce": "4.0.8",
    "lodash.find": "4.6.0",
    "lodash.includes": "4.3.0",
    "lodash.max": "4.0.1",
    "lodash.mean": "4.1.0",
    "lodash.min": "4.0.1",
    "lodash.some": "4.6.0",
    "lodash.sum": "4.0.2",
    "marker-animate-unobtrusive": "0.2.8",
    "mayre": "3.2.3",
    "mobile-detect": "1.4.2",
    "moment": "2.22.2",
    "moment-duration-format": "2.2.2",
    "object-assign": "4.1.1",
    "rabbit.js": "0.4.4",
    "rc-animate": "2.4.4",
    "rc-tween-one": "2.1.4",
    "react": "16.4.1",
    "react-color": "2.14.1",
    "react-cookies": "0.1.0",
    "react-d3-speedometer": "0.3.2",
    "react-dom": "16.4.1",
    "react-google-maps": "9.4.5",
    "react-howler": "3.7.3",
    "react-intl": "2.4.0",
    "react-intl-redux": "2.0.0",
    "react-liquid-gauge": "1.2.4",
    "react-moment": "0.7.9",
    "react-redux": "5.0.7",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "react-stomp": "3.2.0",
    "react-table": "6.8.6",
    "recompose": "0.27.1",
    "redux": "4.0.0",
    "redux-auth-wrapper": "2.0.3",
    "redux-persist": "5.10.0",
    "redux-thunk": "2.3.0",
    "string-template": "1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "8.6.5",
    "babel-plugin-import": "1.8.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "browser-sync": "2.24.5",
    "chalk": "2.4.1",
    "concurrently": "3.6.0",
    "connect-history-api-fallback": "1.5.0",
    "coveralls": "3.0.2",
    "css-loader": "1.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.0.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-watch": "4.0.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "hard-source-webpack-plugin": "0.10.1",
    "history": "4.7.2",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.3.0",
    "jest-cli": "23.3.0",
    "json-loader": "0.5.7",
    "mockdate": "2.0.2",
    "node": "10.6.0",
    "node-sass": "4.9.1",
    "opn-cli": "3.1.0",
    "postcss-loader": "2.1.5",
    "prompt": "1.0.0",
    "prop-types": "15.6.2",
    "raf": "3.4.0",
    "react-hot-loader": "4.3.3",
    "react-test-renderer": "16.4.1",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "replace": "1.0.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.3",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.15.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2",
    "webpack-md5-hash": "0.0.6"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "redux",
    "flux",
    "boilerplate",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js",
      "\\.(css)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "env",
          "react",
          "stage-1"
        ],
        "plugins": [
          "transform-class-properties",
          [
            "import",
            {
              "libraryName": "antd",
              "style": "css"
            },
            {
              "libraryName": "ant-design-pro",
              "style": "css"
            }
          ]
        ]
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "es2015": {
                "modules": false
              },
              "targets": {
                "ie": 9,
                "uglify": true
              }
            }
          ],
          "react",
          "stage-1"
        ],
        "plugins": [
          "react-hot-loader/babel",
          "transform-react-constant-elements",
          "transform-react-remove-prop-types",
          "transform-class-properties",
          [
            "import",
            {
              "libraryName": "antd",
              "style": "css"
            },
            {
              "libraryName": "ant-design-pro",
              "style": "css"
            }
          ]
        ]
      },
      "test": {
        "presets": [
          "env",
          "react",
          "stage-1"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "globals": {}
  }
}
